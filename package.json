{
  "name": "docs-ts-lambda",
  "version": "1.0.0",
  "description": "Files API",
  "main": "serverless.ts",
  "scripts": {
    "clean": "rimraf .esbuild .serverless .nyc_output coverage build output/*",
    "lint": "tslint --project . --format verbose",
    "build": "serverless package",
    "cleanBuild": "yarn clean && yarn build",
    "pm2start": "pm2 start",
    "pm2kill": "pm2 kill",
    "logs": "pm2 logs",
    "pretest": "yarn cleanBuild && pm2 start",
    "test": "yarn integTest || echo \"Finished Test\"",
    "posttest": "yarn pm2kill",
    "integTest": "jest",
    "preIntegTest": "SLS_DEBUG=* serverless offline start --stage test",
    "deploy": "yarn cleanBuild && SLS_DEBUG=* serverless deploy --verbose --stage prod",
    "remove": "SLS_DEBUG=* serverless remove --verbose --stage prod"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "3.164.0",
    "@aws-sdk/client-dynamodb": "3.159.0",
    "@aws-sdk/client-s3": "3.159.0",
    "@aws-sdk/client-textract": "3.159.0",
    "@aws-sdk/lib-dynamodb": "3.159.0",
    "@aws-sdk/s3-request-presigner": "3.159.0",
    "@middy/core": "2.5.3",
    "@middy/http-json-body-parser": "2.5.3",
    "aws-jwt-verify": "3.1.0",
    "crypto": "1.0.1",
    "fluent-ffmpeg": "2.1.2",
    "fs-extra": "10.1.0",
    "jimp": "0.16.1",
    "name2mime": "1.0.1",
    "uuid": "8.3.2",
    "validate.js": "0.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-typescript": "7.16.0",
    "@serverless/typescript": "3.21.0",
    "@types/aws-lambda": "8.10.71",
    "@types/fluent-ffmpeg": "2.1.20",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.25",
    "axios": "0.27.2",
    "babel-jest": "27.3.1",
    "cross-env": "7.0.3",
    "esbuild": "0.14.11",
    "jest": "27.3.1",
    "json-schema-to-ts": "1.5.0",
    "light-async-polling": "1.0.2",
    "pm2": "5.2.0",
    "rimraf": "3.0.2",
    "serverless": "3.21.0",
    "serverless-esbuild": "1.23.3",
    "serverless-offline": "8.5.0",
    "serverless-s3-local": "0.6.22",
    "serverless-dynamodb-local": "0.2.40",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.3",
    "typescript": "4.1.3"
  },
  "license": "MIT"
}
